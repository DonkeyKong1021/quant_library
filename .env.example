# Database Connection URL
# PostgreSQL connection string
# Format: postgresql://username:password@host:port/database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/quant_library

# Data source configuration
# Options: yahoo, alpha_vantage, polygon, iex_cloud
DEFAULT_DATA_SOURCE=yahoo

# Alpha Vantage API
# Get your API key from https://www.alphavantage.co/support/#api-key
ALPHA_VANTAGE_API_KEY=

# Polygon.io API
# Get your API key from https://polygon.io/dashboard/signup
POLYGON_API_KEY=

# IEX Cloud API (Note: IEX Cloud was shut down on August 31, 2024)
# This is included for historical reference only
IEX_CLOUD_API_KEY=
